local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Tapping Legends X Script By SpenGUI#9616", HidePremium = false, SaveConfig = true, ConfigFolder = "Tapping Legends X By SpenGUI"})

OrionLib:MakeNotification({
	Name = "Script Online!",
	Content = "Well there you go folks!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab1 = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://8754597954",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Other",
	Icon = "rbxassetid://",
	PremiumOnly = false
})

local Tab3 = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://",
	PremiumOnly = false
})

local Tab4 = Window:MakeTab({
	Name = "Credits",
	Icon = "rbxassetid://",
	PremiumOnly = false
})

--- tab 1

Tab1:AddToggle({
    Name = "Auto Tap",
    Default = false,
    Callback = function(Value1)
        AutoTap = Value1
        if AutoTap then
            doTap()
            wait()
            else
                AutoTap = false
        end   
    end   
})

Tab1:AddDropdown({
	Name = "Select Rebrith",
	Default = "1",
	Options = {"Spawn", },
	Callback = function(Value)
		SelectedWorld = Value
	end    
})



--- tab 2

Tab2:AddButton({
	Name = "EngoSpy",
	Callback = function()
        local settings = {
            saveCalls = false,
            maxCallsSaved = 1000,
            saveOnlyLastCall = true,
            maxTableDepth = 100,
            minimizeBind = Enum.KeyCode.RightAlt,
            blacklistedNames = {}
         }
         loadstring(game:HttpGet("https://raw.githubusercontent.com/joeengo/engospy/main/source.lua"))(settings)
  	end    
})

local jumpHight = "110000"
local walkSpeed = ""

Tab2:AddSlider({
	Name = "WalkSpeed Changer",
	Min = 0,
	Max = 100,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(Value)
		walkSpeed = Value
	end    
})

Tab2:AddSlider({
	Name = "Jump Hight changer",
	Min = 0,
	Max = 1000,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "jumpHight",
	Callback = function(Value)
		jumpHight = Value
	end    
})

spawn(function ()
	while true do
		local LPHR = game.Players.LocalPlayer.Character.Humanoid
		local Humanoid = player.Character:WaitForChild('Humanoid')
		Humanoid.WalkSpeed = walkSpeed
		LPHR.JumpPower = jumpHight
		print(jumpHight)
		wait()
	end
end)
